.c-from-detail .rows {
    margin-bottom: 10px;
}

.c-from-detail .item {
    float: left;
    width: 33.3%;
    color: #666;
    font-size: 12px;
}

.c-from-detail .label {
    display: inline-block;
    width: 130px;
    text-align: right;
}

.c-from-detail .con {
    display: inline-block;
    padding-left: 20px;
}

.c-from-detail .exclusive {
    float: none;
    width: 100%;
}

.c-from-detail .exclusive .con {
    line-height: 18px;
}